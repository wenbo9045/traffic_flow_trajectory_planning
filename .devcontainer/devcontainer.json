{
  "name": "Edit-Only ROS Project",
  "image": "ubuntu:20.04",
  "features": {
    "ghcr.io/devcontainers/features/python:1": {
      "version": "3.9"
    }
  },
  "extensions": [
    "ms-python.python",
    "ms-vscode.cpptools",
    "ms-iot.vscode-ros"
  ],
  "settings": {
    "python.defaultInterpreterPath": "/usr/bin/python3"
  }
}
